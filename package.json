{
    "private": true,
    "name": "usesashi_app",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev",
        "lint": "turbo lint",
        "test": "turbo run test",
        "publish": "turbo publish",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "release": "yarn build && changeset publish"
    },
    "devDependencies": {
        "@repo/eslint-config": "*",
        "@repo/typescript-config": "*",
        "prettier": "^3.2.5",
        "turbo": ">=2.1.0"
    },
    "engines": {
        "node": ">=18"
    },
    "publishConfig": {
        "access": "public"
    },
    "packageManager": "yarn@1.22.19",
    "version": "1.0.0",
    "dependencies": {
        "recharts": "^2.15.3"
    }
}
