{
    "name": "@sashimo/cli",
    "version": "3.5.0",
    "description": "CLI tool for installing and setting up Sashi in Node.js and Express projects",
    "main": "dist-simple/simple-index.js",
    "bin": {
        "sashi": "dist-simple/simple-index.js",
        "create-sashi": "dist-simple/simple-index.js"
    },
    "scripts": {
        "build": "npx tsc src/simple-index.ts --outDir dist-simple --target ES2020 --module CommonJS --esModuleInterop && chmod +x dist-simple/simple-index.js",
        "dev": "tsc --watch",
        "test": "jest --selectProjects unit --passWithNoTests",
        "test:safe": "jest --selectProjects unit --passWithNoTests",
        "test:e2e": "jest --selectProjects e2e --passWithNoTests",
        "test:all": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "prepublishOnly": "npm run build && npm run test:safe"
    },
    "keywords": [
        "sashi",
        "cli",
        "setup",
        "install",
        "nodejs",
        "express",
        "admin",
        "ai"
    ],
    "author": "Sashi Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/UseSashiMoto/usesashi.git",
        "directory": "packages/sashi-cli"
    },
    "homepage": "https://docs.usesashi.com",
    "bugs": {
        "url": "https://github.com/UseSashiMoto/usesashi/issues"
    },
    "dependencies": {
        "axios": "^1.6.0",
        "chalk": "^4.1.2",
        "commander": "^11.0.0",
        "execa": "^5.1.1",
        "fs-extra": "^11.0.0",
        "inquirer": "^8.2.6",
        "ora": "^5.4.1",
        "semver": "^7.5.4"
    },
    "devDependencies": {
        "typescript": "^5.0.0",
        "jest": "^29.0.0",
        "@types/jest": "^29.0.0",
        "@types/node": "^20.0.0",
        "@types/fs-extra": "^11.0.0",
        "@types/inquirer": "^8.2.6",
        "ts-jest": "^29.0.0",
        "node-fetch": "^2.6.12",
        "@types/node-fetch": "^2.6.4"
    },
    "files": [
        "dist-simple/**/*",
        "README.md",
        "CHANGELOG.md",
        "LICENSE"
    ]
}
